[uwsgi]
socket = 0.0.0.0:1050
http = :1000
wsgi-file = /app/tfe_keeper/tfe_keeper.py
callable = app
processes = 16
threads = 2
listen = 100
memory-report = True
enable-threads = True
disable-logging = True
stats = 127.0.0.1:9191
logger = file:/app/logs/tfe-uwsgi-@(exec://date +%%Y-%%m-%%d).log
touch-reload=/app/tmp/reload
pidfile2 = /app/uwsgi.pid
#logformat = i am a logline reporting "%(method) %(uri) %(proto)" returning with status %(status)
#log-format = [pid: %(pid)|app: -|req: -/-] %(addr) (%(user)) {%(vars) vars in %(pktsize) bytes} [%(ctime)] %(method) %(uri) => generated %(rsize) bytes in %(msecs) msecs (%(proto) %(status)) %(headers) headers in %(hsize) bytes (%(switches) switches on runtime %(runtime))
